<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".fragment.CPRStandardFragment">

    <View
        android:id="@+id/view"
        android:layout_width="5dp"
        android:layout_height="16dp"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/border_dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_6"
        android:text="CPR评分配置"
        android:textColor="@color/white"
        android:textSize="@dimen/text_16"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <TextView
        android:id="@+id/tvMsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_20"
        android:text="按压深度输入范围：整数1～10"
        android:textColor="@color/color_text_selected"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10">

                <TextView
                    android:id="@+id/tv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="28dp"
                    android:layout_marginTop="8dp"
                    android:ems="7"
                    android:text="按压深度（cm）"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textview1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tv1"
                    app:layout_constraintStart_toEndOf="@+id/tv1"
                    app:layout_constraintTop_toTopOf="@+id/tv1" />

                <TextView
                    android:id="@+id/tv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_30"
                    android:layout_marginTop="@dimen/dp_30"
                    android:ems="7"
                    android:text="按压频率（cpm）"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv1" />

                <TextView
                    android:id="@+id/textview2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tv2"
                    app:layout_constraintStart_toEndOf="@+id/tv2"
                    app:layout_constraintTop_toTopOf="@+id/tv2" />

                <TextView
                    android:id="@+id/tv3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_30"
                    android:layout_marginTop="@dimen/dp_30"
                    android:ems="7"
                    android:text="按压中断（ms）"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv2" />

                <TextView
                    android:id="@+id/textview3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tv3"
                    app:layout_constraintStart_toEndOf="@+id/tv3"
                    app:layout_constraintTop_toTopOf="@+id/tv3" />

                <TextView
                    android:id="@+id/tv4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_30"
                    android:layout_marginTop="@dimen/dp_30"
                    android:ems="7"
                    android:text="潮气量（ml）"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv3" />

                <TextView
                    android:id="@+id/textview4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tv4"
                    app:layout_constraintStart_toEndOf="@+id/tv4"
                    app:layout_constraintTop_toTopOf="@+id/tv4" />

                <TextView
                    android:id="@+id/tv5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_30"
                    android:layout_marginTop="@dimen/dp_30"
                    android:ems="7"
                    android:text="潮气频率（vpm）"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv4" />

                <TextView
                    android:id="@+id/textview5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tv5"
                    app:layout_constraintStart_toEndOf="@+id/tv5"
                    app:layout_constraintTop_toTopOf="@+id/tv5" />

                <EditText
                    android:id="@+id/etDepth"
                    style="@style/EditTextStyle"

                    app:layout_constraintBottom_toBottomOf="@+id/textview1"
                    app:layout_constraintStart_toEndOf="@+id/textview1"
                    app:layout_constraintTop_toTopOf="@+id/textview1" />

                <EditText
                    android:id="@+id/etDepthFrequency"
                    style="@style/EditTextStyle"

                    app:layout_constraintBottom_toBottomOf="@+id/textview2"
                    app:layout_constraintStart_toEndOf="@+id/textview2"
                    app:layout_constraintTop_toTopOf="@+id/textview2" />

                <EditText
                    android:id="@+id/etTidalVolume"
                    style="@style/EditTextStyle"
                    app:layout_constraintBottom_toBottomOf="@+id/textview4"
                    app:layout_constraintStart_toEndOf="@+id/textview4"
                    app:layout_constraintTop_toTopOf="@+id/textview4"
                    app:layout_constraintVertical_bias="1.0" />

                <EditText
                    android:id="@+id/etTidalFrequency"
                    style="@style/EditTextStyle"
                    app:layout_constraintBottom_toBottomOf="@+id/textview5"
                    app:layout_constraintStart_toEndOf="@+id/textview5"
                    app:layout_constraintTop_toTopOf="@+id/textview5" />

                <TextView
                    android:id="@+id/tvd1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text="到"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_14"
                    app:layout_constraintBottom_toBottomOf="@+id/etDepth"
                    app:layout_constraintStart_toEndOf="@+id/etDepth"
                    app:layout_constraintTop_toTopOf="@+id/etDepth" />

                <TextView
                    android:id="@+id/tvd2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text="到"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_14"
                    app:layout_constraintBottom_toBottomOf="@+id/etDepthFrequency"
                    app:layout_constraintStart_toEndOf="@+id/etDepthFrequency"
                    app:layout_constraintTop_toTopOf="@+id/etDepthFrequency" />

                <TextView
                    android:id="@+id/tvd3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text="    不能超过"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_14"
                    app:layout_constraintBottom_toBottomOf="@+id/textview3"
                    app:layout_constraintStart_toEndOf="@+id/textview3"
                    app:layout_constraintTop_toTopOf="@+id/textview3" />

                <TextView
                    android:id="@+id/tvd4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text="到"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_14"
                    app:layout_constraintBottom_toBottomOf="@+id/etTidalVolume"
                    app:layout_constraintStart_toEndOf="@+id/etTidalVolume"
                    app:layout_constraintTop_toTopOf="@+id/etTidalVolume" />

                <TextView
                    android:id="@+id/tvd5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text="到"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_14"
                    app:layout_constraintBottom_toBottomOf="@+id/etTidalFrequency"
                    app:layout_constraintStart_toEndOf="@+id/etTidalFrequency"
                    app:layout_constraintTop_toTopOf="@+id/etTidalFrequency" />

                <EditText
                    android:id="@+id/etDepthEnd"
                    style="@style/EditTextStyle"
                    android:layout_width="58dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/tvd1"
                    app:layout_constraintStart_toEndOf="@+id/tvd1"
                    app:layout_constraintTop_toTopOf="@+id/tvd1"
                    app:layout_constraintVertical_bias="0.444" />

                <EditText
                    android:id="@+id/etDepthFrequencyEnd"
                    style="@style/EditTextStyle"
                    android:layout_marginStart="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="@+id/tvd2"
                    app:layout_constraintStart_toEndOf="@+id/tvd2"
                    app:layout_constraintTop_toTopOf="@+id/tvd2" />

                <EditText
                    android:id="@+id/etInterrupt"
                    style="@style/EditTextStyle"
                    app:layout_constraintBottom_toBottomOf="@+id/tvd3"
                    app:layout_constraintStart_toStartOf="@+id/etDepthFrequencyEnd"
                    app:layout_constraintTop_toTopOf="@+id/tvd3" />

                <EditText
                    android:id="@+id/etTidalVolumeEnd"
                    style="@style/EditTextStyle"
                    app:layout_constraintBottom_toBottomOf="@+id/tvd4"
                    app:layout_constraintStart_toStartOf="@+id/etInterrupt"
                    app:layout_constraintTop_toTopOf="@+id/tvd4"
                    app:layout_constraintVertical_bias="0.555" />

                <EditText
                    android:id="@+id/etTidalFrequencyEnd"
                    style="@style/EditTextStyle"
                    android:layout_marginStart="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="@+id/tvd5"
                    app:layout_constraintStart_toEndOf="@+id/tvd5"
                    app:layout_constraintTop_toTopOf="@+id/tvd5" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_40">

                <View
                    android:id="@+id/view1"
                    android:layout_width="5dp"
                    android:layout_height="16dp"
                    android:layout_marginStart="@dimen/dp_20"
                    android:background="@drawable/border_dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text="考核设置"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/view1"
                    app:layout_constraintStart_toEndOf="@+id/view1"
                    app:layout_constraintTop_toTopOf="@+id/view1" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/viewline">

                <TextView
                    android:id="@+id/tvProcess1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_30"
                    android:ems="7"
                    android:text="操作时长（m:s）"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textview6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tvProcess1"
                    app:layout_constraintStart_toEndOf="@+id/tvProcess1"
                    app:layout_constraintTop_toTopOf="@+id/tvProcess1" />

                <EditText
                    android:id="@+id/etTime"
                    style="@style/EditTextStyle2"
                    android:layout_marginEnd="@dimen/dp_30"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintlayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/viewline">

                <TextView
                    android:id="@+id/tvProcess2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_30"
                    android:ems="7"
                    android:text="循环次数（次）"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintBottom_toBottomOf="@+id/etCycles"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textview7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tvProcess2"
                    app:layout_constraintStart_toEndOf="@+id/tvProcess2"
                    app:layout_constraintTop_toTopOf="@+id/tvProcess2" />

                <EditText
                    android:id="@+id/etCycles"
                    style="@style/EditTextStyle"
                    android:layout_marginEnd="@dimen/dp_30"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintlayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/viewline">

                <TextView
                    android:id="@+id/tvProcess3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_30"
                    android:ems="7"
                    android:text="按压通气比列"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_16"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textview8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" ："
                    android:textColor="#ffffffff"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tvProcess3"
                    app:layout_constraintStart_toEndOf="@+id/tvProcess3"
                    app:layout_constraintTop_toTopOf="@+id/tvProcess3" />

                <EditText
                    android:id="@+id/etCompression"
                    style="@style/EditTextStyle"
                    android:layout_marginEnd="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="@+id/etCompression2"
                    app:layout_constraintEnd_toStartOf="@+id/textView3"
                    app:layout_constraintTop_toTopOf="@+id/etCompression2" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="10dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_18"
                    app:layout_constraintBottom_toBottomOf="@+id/etCompression2"
                    app:layout_constraintEnd_toStartOf="@+id/etCompression2"
                    app:layout_constraintTop_toTopOf="@+id/etCompression2" />

                <EditText
                    android:id="@+id/etCompression2"
                    style="@style/EditTextStyle"
                    android:layout_marginEnd="28dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>