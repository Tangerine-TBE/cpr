<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/clContent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/dp_10"
        android:background="@color/white">

    <TextView
            android:id="@+id/tvTrain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="18sp"
            android:textColor="@color/black"
            android:text="完美复苏CPR操作报告"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    <TextView
            android:id="@+id/tvScoreSetting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="分数设定： "
            android:textSize="12sp"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/dp_10"
            app:layout_constraintTop_toBottomOf="@+id/tvTrain" />

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/border_1dp"
            android:layout_marginTop="@dimen/dp_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvScoreSetting"
            tools:layout_editor_absoluteX="10dp"
            app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@drawable/border_1dp_bg"
                android:id="@+id/constraintLayout4">

            <View
                    android:id="@+id/view"
                    android:layout_width="3dp"
                    android:layout_height="12dp"
                    android:layout_marginStart="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@color/black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_13"
                    android:textStyle="bold"
                    android:layout_marginStart="@dimen/dp_6"
                    android:text="基本信息"
                    app:layout_constraintStart_toStartOf="@+id/view"
                    app:layout_constraintTop_toTopOf="@+id/view"
                    app:layout_constraintBottom_toBottomOf="@+id/view" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout1"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/border_1dp"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

            <TextView
                    android:id="@+id/tvName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tvTime"
                    android:text="学员姓名：" />

            <TextView
                    android:id="@+id/tvTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:text="操作时间："
                    app:layout_constraintStart_toEndOf="@+id/tvName" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout1">

            <TextView
                    android:id="@+id/tvCycle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tvOperationTime"
                    android:text="循环次数：" />

            <TextView
                    android:id="@+id/tvOperationTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:text="操作时长："
                    app:layout_constraintStart_toEndOf="@+id/tvCycle" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/border_1dp"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
                android:id="@+id/constraintLayout5">

            <TextView
                    android:id="@+id/tvModel"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tvCycleSetting"
                    android:text="操作模式：" />

            <TextView
                    android:id="@+id/tvCycleSetting"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:text="循环定义："
                    app:layout_constraintStart_toEndOf="@+id/tvModel" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <View
                android:layout_width="1dp"
                android:layout_height="0dp"
                android:background="#909090"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout4"
                app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
                app:layout_constraintEnd_toEndOf="@+id/constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/constraintLayout2" />

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/border_1dp_bg"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout5"
                android:id="@+id/constraintLayout6">

            <View
                    android:id="@+id/view2"
                    android:layout_width="3dp"
                    android:layout_height="12dp"
                    android:layout_marginStart="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@color/black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_13"
                    android:textStyle="bold"
                    android:layout_marginStart="@dimen/dp_6"
                    android:text="CPR成绩"
                    app:layout_constraintStart_toStartOf="@+id/view2"
                    app:layout_constraintTop_toTopOf="@+id/view2"
                    app:layout_constraintBottom_toBottomOf="@+id/view2" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <RatingBar
                android:id="@+id/ratingBar"
                style="@style/foodRatingBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:isIndicator="false"
                android:numStars="5"
                android:rating="0"
                android:stepSize="0.5"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout6" />

        <TextView
                android:id="@+id/tvScore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:textColor="@color/color_37B48B"
                android:textSize="28sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/ratingBar"
                app:layout_constraintStart_toStartOf="@+id/ratingBar"
                app:layout_constraintTop_toBottomOf="@+id/ratingBar" />

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_marginBottom="@dimen/dp_2"
                android:text="分"
                android:textColor="@color/color_text_normal"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/tvScore"
                app:layout_constraintStart_toEndOf="@+id/tvScore"
                android:id="@+id/textView12" />

        <TextView
                android:id="@+id/tvPressScore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                android:text="按压得分"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvScore" />

        <TextView
                android:id="@+id/tv4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_12"
                android:text="中断扣分"
                android:textColor="@color/color_text_selected"
                android:textStyle="bold"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvScore" />

        <TextView
                android:id="@+id/tv5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_12"
                android:text="通气得分"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="12sp"
                android:layout_marginEnd="@dimen/dp_10"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvScore" />

        <TextView
                android:id="@+id/tv6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="流程得分"
                android:textColor="@color/black"
                android:layout_marginTop="@dimen/dp_4"
                android:textStyle="bold"
                android:textSize="12sp"
                app:layout_constraintTop_toBottomOf="@+id/tvPressScore"
                app:layout_constraintStart_toStartOf="@+id/tvPressScore" />

        <CheckBox
                android:id="@+id/checkBox1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:layout_marginTop="@dimen/dp_10"
                android:checked="false"
                android:text="检查环境"
                android:layout_marginStart="@dimen/dp_6"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                app:layout_constraintEnd_toStartOf="@+id/checkBox2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv6" />

        <CheckBox
                android:id="@+id/checkBox2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="检查意识"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                app:layout_constraintEnd_toStartOf="@+id/checkBox3"
                app:layout_constraintTop_toTopOf="@+id/checkBox1"
                app:layout_constraintStart_toEndOf="@+id/checkBox1"
                app:layout_constraintBottom_toBottomOf="@+id/checkBox1" />

        <CheckBox
                android:id="@+id/checkBox3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="检查呼吸"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                app:layout_constraintStart_toEndOf="@+id/checkBox2"
                app:layout_constraintTop_toTopOf="@+id/checkBox2"
                app:layout_constraintEnd_toStartOf="@+id/checkBox4" />

        <CheckBox
                android:id="@+id/checkBox4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="检查脉搏"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                app:layout_constraintEnd_toStartOf="@+id/checkBox5"
                app:layout_constraintStart_toEndOf="@+id/checkBox3"
                app:layout_constraintTop_toTopOf="@+id/checkBox3" />

        <CheckBox
                android:id="@+id/checkBox5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="检查体位"
                app:layout_constraintStart_toEndOf="@+id/checkBox4"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                android:layout_marginEnd="@dimen/dp_6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/checkBox4" />

        <CheckBox
                android:id="@+id/checkBox6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="清理口腔"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                android:layout_marginTop="@dimen/dp_10"
                app:layout_constraintStart_toStartOf="@+id/checkBox1"
                app:layout_constraintEnd_toStartOf="@+id/checkBox7"
                app:layout_constraintTop_toBottomOf="@+id/checkBox1" />

        <CheckBox
                android:id="@+id/checkBox7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="拨打120"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                app:layout_constraintEnd_toStartOf="@+id/checkBox8"
                app:layout_constraintStart_toEndOf="@+id/checkBox6"
                app:layout_constraintTop_toTopOf="@+id/checkBox6" />

        <CheckBox
                android:id="@+id/checkBox8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="寻求帮助"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                app:layout_constraintEnd_toStartOf="@+id/checkBox9"
                app:layout_constraintStart_toEndOf="@+id/checkBox7"
                app:layout_constraintTop_toTopOf="@+id/checkBox7" />

        <CheckBox
                android:id="@+id/checkBox9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="体外除颤"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                app:layout_constraintEnd_toStartOf="@+id/checkBox10"
                app:layout_constraintTop_toTopOf="@+id/checkBox8"
                app:layout_constraintStart_toEndOf="@+id/checkBox8" />

        <CheckBox
                android:id="@+id/checkBox10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/checkbox_selector"
                android:checked="false"
                android:text="完成评估"
                android:textColor="@color/black"
                android:textSize="@dimen/text_14"
                android:layout_marginEnd="@dimen/dp_6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/checkBox9"
                app:layout_constraintStart_toEndOf="@+id/checkBox9" />

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/border_1dp_bg"
                android:layout_marginTop="@dimen/dp_6"
                app:layout_constraintTop_toBottomOf="@+id/checkBox6"
                android:id="@+id/constraintLayout7">

            <View
                    android:id="@+id/view3"
                    android:layout_width="3dp"
                    android:layout_height="12dp"
                    android:layout_marginStart="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@color/black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_13"
                    android:textStyle="bold"
                    android:layout_marginStart="@dimen/dp_6"
                    android:text="操作细节"
                    app:layout_constraintStart_toStartOf="@+id/view3"
                    app:layout_constraintTop_toTopOf="@+id/view3"
                    app:layout_constraintBottom_toBottomOf="@+id/view3" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
                android:id="@+id/iv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_48"
                android:layout_marginTop="@dimen/dp_5"
                android:src="@mipmap/icon_wm_chart_heart_red"
                app:layout_constraintEnd_toStartOf="@+id/tvCycleCount"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout7" />

        <TextView
                android:id="@+id/tvCycleCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/progressbar_circle"
                android:gravity="center"
                android:text="0"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/iv1"
                app:layout_constraintBottom_toBottomOf="@+id/iv1" />

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_48"
                android:src="@mipmap/icon_wm_chart_lung_red"
                app:layout_constraintStart_toEndOf="@+id/tvCycleCount"
                app:layout_constraintTop_toTopOf="@+id/iv1"
                android:id="@+id/imageView6" />

        <TextView
                android:id="@+id/tvLungCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(错误/总数)"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@+id/iv1"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/iv1" />

        <TextView
                android:id="@+id/tvPrCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(错误/总数)"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_marginStart="@dimen/dp_10"
                app:layout_constraintStart_toEndOf="@+id/tvLungCount"
                app:layout_constraintTop_toTopOf="@+id/tvLungCount" />

        <TextView
                android:id="@+id/tvHeartCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(错误/总数)"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_marginTop="@dimen/dp_10"
                app:layout_constraintStart_toEndOf="@+id/tvCycleCount"
                app:layout_constraintTop_toBottomOf="@+id/imageView6" />

        <TextView
                android:id="@+id/tvQyCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(错误/总数)"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_marginStart="@dimen/dp_10"
                app:layout_constraintStart_toEndOf="@+id/tvHeartCount"
                app:layout_constraintTop_toTopOf="@+id/tvHeartCount" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>