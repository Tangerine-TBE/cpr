<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/theme_color">

    <include
            android:id="@+id/top"
            layout="@layout/layout_top"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_48"
            app:layout_constraintTop_toTopOf="parent" />

    <androidx.core.widget.NestedScrollView
            android:id="@+id/nsContent"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/bottom"
            app:layout_constraintTop_toBottomOf="@+id/top">

        <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

            <include
                    android:id="@+id/layoutCheck"
                    layout="@layout/layout_check_result"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clResult"
                    android:background="@color/theme_color"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayout"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:background="#22231D"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                    <TextView
                            android:id="@+id/tvTrainName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_20"
                            android:textColor="@color/white"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/tvTrain"
                            app:layout_constraintHorizontal_chainStyle="spread_inside"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                            android:id="@+id/tvTrain"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawablePadding="4dp"
                            android:gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:textSize="16sp"
                            android:text="训练"
                            app:layout_constraintBottom_toBottomOf="@+id/tvTrainName"
                            app:layout_constraintEnd_toStartOf="@+id/tvTime"
                            app:layout_constraintStart_toEndOf="@+id/tvTrainName"
                            app:layout_constraintTop_toTopOf="@+id/tvTrainName" />

                    <TextView
                            android:id="@+id/tvTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/dp_32"
                            android:gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:textSize="@dimen/text_14"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/tvTrain"
                            app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.Group
                        android:id="@+id/gruops"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:constraint_referenced_ids="constraintLayout" />

                <View
                        android:id="@+id/view"
                        android:layout_width="5dp"
                        android:layout_height="16dp"
                        android:layout_marginStart="@dimen/dp_20"
                        android:layout_marginTop="@dimen/dp_16"
                        android:background="@drawable/border_dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

                <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_6"
                        android:text="操作明细"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_16"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/view"
                        app:layout_constraintStart_toEndOf="@+id/view"
                        app:layout_constraintTop_toTopOf="@+id/view" />

                <ImageView
                        android:id="@+id/iv1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_48"
                        android:src="@mipmap/icon_wm_chart_heart_red"
                        app:layout_constraintEnd_toStartOf="@+id/tvCycleCount"
                        app:layout_constraintTop_toTopOf="@+id/tvCycleCount" />

                <TextView
                        android:id="@+id/tvCycleCount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_16"
                        android:background="@mipmap/icon_wm_start_level_cycle"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_37B48B"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView2" />

                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_48"
                        android:src="@mipmap/icon_wm_chart_lung_red"
                        app:layout_constraintStart_toEndOf="@+id/tvCycleCount"
                        app:layout_constraintTop_toTopOf="@+id/tvCycleCount" />

                <androidx.cardview.widget.CardView
                        android:id="@+id/cardView1"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_44"
                        android:layout_marginStart="@dimen/dp_16"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginEnd="@dimen/dp_8"
                        app:cardBackgroundColor="#B322231D"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="0dp"
                        app:layout_constraintEnd_toStartOf="@+id/cardView2"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/iv1"
                        app:layout_constraintVertical_chainStyle="spread_inside">

                    <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                        <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                    android:id="@+id/tvLungCount"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/color_text_selected"
                                    android:textSize="24sp"
                                    app:layout_constraintEnd_toStartOf="@+id/tvLungTotal"
                                    app:layout_constraintTop_toTopOf="@+id/tvLungTotal" />

                            <TextView
                                    android:id="@+id/tvLungTotal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="/0"
                                    android:textColor="#ffffffff"
                                    android:textSize="24sp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                        </androidx.appcompat.widget.LinearLayoutCompat>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                        android:id="@+id/cardView2"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_44"
                        android:layout_marginStart="@dimen/dp_8"
                        android:layout_marginEnd="@dimen/dp_16"
                        app:cardBackgroundColor="#B322231D"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="0dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/cardView1"
                        app:layout_constraintTop_toTopOf="@+id/cardView1">

                    <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                        <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                    android:id="@+id/tvHeartCount"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/color_text_selected"
                                    android:textSize="24sp"
                                    app:layout_constraintEnd_toStartOf="@+id/tvHeartTotal"
                                    app:layout_constraintTop_toTopOf="@+id/tvHeartTotal" />

                            <TextView
                                    android:id="@+id/tvHeartTotal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="/0"
                                    android:textColor="#ffffffff"
                                    android:textSize="24sp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />
                        </androidx.appcompat.widget.LinearLayoutCompat>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                        android:id="@+id/cardView3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_16"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginEnd="@dimen/dp_8"
                        app:cardBackgroundColor="#B322231D"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="0dp"
                        app:layout_constraintEnd_toStartOf="@+id/cardView4"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cardView1"
                        app:layout_constraintVertical_chainStyle="spread_inside">

                    <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingBottom="@dimen/dp_12">

                        <TextView
                                android:id="@+id/textView18"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_16"
                                android:layout_marginTop="@dimen/dp_16"
                                android:text="位置"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_chainStyle="spread_inside" />

                        <TextView
                                android:id="@+id/tv19"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/dp_35"
                                android:text="不足"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="@+id/textView18" />

                        <TextView
                                android:id="@+id/tvLocation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/textView18"
                                app:layout_constraintTop_toBottomOf="@+id/textView18" />

                        <TextView
                                android:id="@+id/tvInsufficient"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/tv19"
                                app:layout_constraintTop_toBottomOf="@+id/tv19" />

                        <View
                                android:id="@+id/viewline1"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#1EFFFFFF"
                                android:layout_marginTop="@dimen/dp_6"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tvInsufficient" />

                        <TextView
                                android:id="@+id/tv21"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dp_6"
                                android:text="过大"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="@+id/tvLocation"
                                app:layout_constraintTop_toBottomOf="@+id/viewline1" />

                        <TextView
                                android:id="@+id/tvPressBig"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/tv21"
                                app:layout_constraintTop_toBottomOf="@+id/tv21" />

                        <TextView
                                android:id="@+id/tv23"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="回弹"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="@+id/tvInsufficient"
                                app:layout_constraintTop_toTopOf="@+id/tv21" />

                        <TextView
                                android:id="@+id/tvRebound"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/tv23"
                                app:layout_constraintTop_toBottomOf="@+id/tv23" />

                        <View
                                android:id="@+id/viewline3"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#1EFFFFFF"
                                android:layout_marginTop="@dimen/dp_6"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tvRebound" />

                        <TextView
                                android:id="@+id/tvOutBout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="超次"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                android:layout_marginTop="@dimen/dp_6"
                                app:layout_constraintTop_toBottomOf="@+id/viewline3"
                                app:layout_constraintStart_toStartOf="@+id/tvPressBig" />

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintTop_toBottomOf="@+id/tvOutBout"
                                app:layout_constraintStart_toStartOf="@+id/tvOutBout"
                                android:id="@+id/tvOutBoutCount" />

                        <TextView
                                android:id="@+id/tvSmallBout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="少次"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                android:layout_marginTop="@dimen/dp_6"
                                app:layout_constraintTop_toBottomOf="@+id/viewline3"
                                app:layout_constraintEnd_toEndOf="@+id/tv23" />

                        <TextView
                                android:id="@+id/tvSmallBoutCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintTop_toBottomOf="@+id/tvSmallBout"
                                app:layout_constraintEnd_toEndOf="@+id/tvSmallBout" />

                        <androidx.constraintlayout.widget.Group
                                android:id="@+id/groupPr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:visibility="gone"
                                app:constraint_referenced_ids="tvSmallBout, tvSmallBoutCount, tvOutBout, tvOutBoutCount, viewline3" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                        android:id="@+id/cardView4"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_8"
                        android:layout_marginEnd="@dimen/dp_16"
                        app:cardBackgroundColor="#B322231D"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="0dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/cardView3"
                        app:layout_constraintTop_toTopOf="@+id/cardView3">

                    <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingBottom="@dimen/dp_12">

                        <TextView
                                android:id="@+id/tv1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_16"
                                android:layout_marginTop="@dimen/dp_16"
                                android:text="气道"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_chainStyle="spread_inside" />

                        <TextView
                                android:id="@+id/tv2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/dp_35"
                                android:text="不足"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="@+id/tv1" />

                        <TextView
                                android:id="@+id/tvAirway"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/tv1"
                                app:layout_constraintTop_toBottomOf="@+id/tv1" />

                        <TextView
                                android:id="@+id/tvCInsufficient"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/tv2"
                                app:layout_constraintTop_toBottomOf="@+id/tv2" />

                        <View
                                android:id="@+id/viewLine2"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#1EFFFFFF"
                                android:layout_marginTop="@dimen/dp_6"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tvCInsufficient" />

                        <TextView
                                android:id="@+id/tv5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dp_6"
                                android:text="过大"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="@+id/tvAirway"
                                app:layout_constraintTop_toBottomOf="@+id/viewLine2" />

                        <TextView
                                android:id="@+id/tvBLowBig"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/tv5"
                                app:layout_constraintTop_toBottomOf="@+id/tv5" />

                        <TextView
                                android:id="@+id/tv7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="进胃"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="@+id/tvCInsufficient"
                                app:layout_constraintTop_toTopOf="@+id/tv5" />

                        <TextView
                                android:id="@+id/tvIntoStomach"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dp_12"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintStart_toStartOf="@+id/tv7"
                                app:layout_constraintTop_toBottomOf="@+id/tv7" />

                        <View
                                android:id="@+id/viewline4"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#1EFFFFFF"
                                android:layout_marginTop="@dimen/dp_6"
                                android:layout_marginStart="@dimen/dp_12"
                                android:layout_marginEnd="@dimen/dp_12"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tvIntoStomach" />

                        <TextView
                                android:id="@+id/tvBlowBout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="超次"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                android:layout_marginTop="@dimen/dp_6"
                                app:layout_constraintTop_toBottomOf="@+id/viewline4"
                                app:layout_constraintStart_toStartOf="@+id/tvBLowBig" />

                        <TextView
                                android:id="@+id/tvBlowBoutCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintTop_toBottomOf="@+id/tvBlowBout"
                                app:layout_constraintStart_toStartOf="@+id/tvBlowBout" />

                        <TextView
                                android:id="@+id/tvBlowSmallBout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="少次"
                                android:textColor="#ffffffff"
                                android:textSize="14sp"
                                android:layout_marginTop="@dimen/dp_6"
                                app:layout_constraintTop_toBottomOf="@+id/viewline4"
                                app:layout_constraintEnd_toEndOf="@+id/tv7" />

                        <TextView
                                android:id="@+id/tvBlowSmallCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0次"
                                android:textColor="@color/color_text_selected"
                                android:textSize="18sp"
                                app:layout_constraintTop_toBottomOf="@+id/tvBlowSmallBout"
                                app:layout_constraintEnd_toEndOf="@+id/tvBlowSmallBout" />

                        <androidx.constraintlayout.widget.Group
                                android:id="@+id/groupQy"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:visibility="gone"
                                app:constraint_referenced_ids="tvBlowBout, tvBlowBoutCount, tvBlowSmallBout, viewline4, tvBlowSmallCount" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <TextView
                        android:id="@+id/tvAverageCount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_30"
                        android:layout_marginTop="@dimen/dp_20"
                        android:text="平均：0次/分"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cardView3" />

                <TextView
                        android:id="@+id/tvBlowAverageCount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_30"
                        android:text="平均：0次/分"
