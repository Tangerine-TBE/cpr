<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/cl_export_Content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:background="@color/white">

    <TextView
            android:id="@+id/tvTrain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="16sp"
            android:textColor="@color/black"
            android:text="@string/export_title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    <TextView
            android:id="@+id/tvScoreSetting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="分数设定： "
            android:textSize="10sp"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/dp_2"
            app:layout_constraintTop_toBottomOf="@+id/tvTrain" />

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/dp_10"
            android:background="@drawable/border_1dp"
            android:layout_marginTop="@dimen/dp_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvScoreSetting"
            tools:layout_editor_absoluteX="4dp"
            app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/dp_2"
                android:paddingBottom="@dimen/dp_2"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@drawable/border_1dp_bg"
                android:id="@+id/constraintLayout4">

            <View
                    android:id="@+id/view"
                    android:layout_width="3dp"
                    android:layout_height="12dp"
                    android:layout_marginStart="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@color/black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_13"
                    android:textStyle="bold"
                    android:layout_marginStart="@dimen/dp_6"
                    android:text="基本信息"
                    app:layout_constraintStart_toStartOf="@+id/view"
                    app:layout_constraintTop_toTopOf="@+id/view"
                    app:layout_constraintBottom_toBottomOf="@+id/view" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout1"
                android:layout_width="match_parent"
                android:layout_height="22dp"
                android:background="@drawable/border_1dp"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

            <TextView
                    android:id="@+id/tvName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_10"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tvTime"
                    android:text="学员姓名："
                    app:layout_constraintVertical_bias="0.333" />

            <TextView
                    android:id="@+id/tvTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_10"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    android:lines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:text="操作时间："
                    app:layout_constraintStart_toEndOf="@+id/tvName" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="match_parent"
                android:layout_height="22dp"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout1">

            <TextView
                    android:id="@+id/tvCycle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_10"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tvOperationTime"
                    android:text="循环次数：" />

            <TextView
                    android:id="@+id/tvOperationTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_10"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:text="操作时长："
                    app:layout_constraintStart_toEndOf="@+id/tvCycle" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="22dp"
                android:background="@drawable/border_1dp"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
                android:id="@+id/constraintLayout5">

            <TextView
                    android:id="@+id/tvModel"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_10"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tvCycleSetting"
                    android:text="操作模式：" />

            <TextView
                    android:id="@+id/tvCycleSetting"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_10"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:text="循环定义："
                    app:layout_constraintStart_toEndOf="@+id/tvModel"
                    app:layout_constraintVertical_bias="0.444" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <View
                android:layout_width="1dp"
                android:layout_height="0dp"
                android:background="#909090"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout4"
                app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
                app:layout_constraintEnd_toEndOf="@+id/constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/constraintLayout2" />

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/constraintLayout6"
                android:visibility="visible"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout5">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="@dimen/dp_2"
                    android:paddingBottom="@dimen/dp_2"
                    android:background="@drawable/border_1dp_bg"
                    app:layout_constraintTop_toTopOf="parent"
                    android:id="@+id/clCheck">

                <View
                        android:id="@+id/view2"
                        android:layout_width="3dp"
                        android:layout_height="12dp"
                        android:layout_marginStart="@dimen/dp_10"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:background="@color/black"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_13"
                        android:textStyle="bold"
                        android:layout_marginStart="@dimen/dp_6"
                        android:text="CPR成绩"
                        app:layout_constraintStart_toStartOf="@+id/view2"
                        app:layout_constraintTop_toTopOf="@+id/view2"
                        app:layout_constraintBottom_toBottomOf="@+id/view2" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <RatingBar
                    android:id="@+id/ratingBar1"
                    style="@style/foodRatingBarRed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:rating="0"
                    android:stepSize="0.5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/clCheck" />

            <RatingBar
                    android:id="@+id/ratingBar2"
                    style="@style/foodRatingBarYellow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:rating="0"
                    android:stepSize="0.5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/clCheck" />

            <RatingBar
                    android:id="@+id/ratingBar3"
                    style="@style/foodRatingBar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:rating="0"
                    android:stepSize="0.5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/clCheck" />

            <TextView
                    android:id="@+id/tvScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textColor="@color/color_37B48B"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/ratingBar1"
                    app:layout_constraintStart_toStartOf="@+id/ratingBar1"
                    app:layout_constraintTop_toBottomOf="@+id/ratingBar1" />

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_4"
                    android:text="分"
                    android:textColor="@color/color_text_normal"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/tvScore"
                    app:layout_constraintStart_toEndOf="@+id/tvScore"
                    android:id="@+id/textView12" />

            <TextView
                    android:id="@+id/tvPressScore1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:text="按压得分"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvScore" />

            <TextView
                    android:id="@+id/tvInterruptScore1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_12"
                    android:text="中断扣分"
                    android:textColor="@color/color_text_selected"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvScore" />

            <TextView
                    android:id="@+id/tvVentilationScore1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_12"
                    android:text="通气得分"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    android:layout_marginEnd="@dimen/dp_10"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvScore" />

            <TextView
                    android:id="@+id/tvProcessScore2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="流程得分"
                    android:textColor="@color/black"
                    android:layout_marginTop="@dimen/dp_4"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    app:layout_constraintTop_toBottomOf="@+id/tvPressScore1"
                    app:layout_constraintStart_toStartOf="@+id/tvPressScore1" />

            <CheckBox
                    android:id="@+id/checkBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="检查环境"
                    android:layout_marginStart="@dimen/dp_6"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvProcessScore2" />

            <CheckBox
                    android:id="@+id/checkBox2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="检查意识"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox3"
                    app:layout_constraintTop_toTopOf="@+id/checkBox1"
                    app:layout_constraintStart_toEndOf="@+id/checkBox1"
                    app:layout_constraintBottom_toBottomOf="@+id/checkBox1" />

            <CheckBox
                    android:id="@+id/checkBox3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="检查呼吸"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    app:layout_constraintStart_toEndOf="@+id/checkBox2"
                    app:layout_constraintTop_toTopOf="@+id/checkBox2"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox4" />

            <CheckBox
                    android:id="@+id/checkBox4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="检查脉搏"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox5"
                    app:layout_constraintStart_toEndOf="@+id/checkBox3"
                    app:layout_constraintTop_toTopOf="@+id/checkBox3" />

            <CheckBox
                    android:id="@+id/checkBox5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="检查体位"
                    app:layout_constraintStart_toEndOf="@+id/checkBox4"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:layout_marginEnd="@dimen/dp_6"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/checkBox4" />

            <CheckBox
                    android:id="@+id/checkBox6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="清理口腔"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:layout_marginStart="@dimen/dp_5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox7"
                    app:layout_constraintTop_toBottomOf="@+id/checkBox1" />

            <CheckBox
                    android:id="@+id/checkBox7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="拨打120"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox8"
                    app:layout_constraintStart_toEndOf="@+id/checkBox6"
                    app:layout_constraintTop_toTopOf="@+id/checkBox6" />

            <CheckBox
                    android:id="@+id/checkBox8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="寻求帮助"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox9"
                    app:layout_constraintStart_toEndOf="@+id/checkBox7"
                    app:layout_constraintTop_toTopOf="@+id/checkBox7" />

            <CheckBox
                    android:id="@+id/checkBox9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="体外除颤"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    app:layout_constraintEnd_toStartOf="@+id/checkBox10"
                    app:layout_constraintTop_toTopOf="@+id/checkBox8"
                    app:layout_constraintStart_toEndOf="@+id/checkBox8" />

            <CheckBox
                    android:id="@+id/checkBox10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_selector"
                    android:checked="false"
                    android:text="完成评估"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_12"
                    android:layout_marginEnd="@dimen/dp_6"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/checkBox9"
                    app:layout_constraintStart_toEndOf="@+id/checkBox9" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/dp_2"
                android:paddingBottom="@dimen/dp_2"
                android:background="@drawable/border_1dp_bg"
                android:layout_marginTop="@dimen/dp_2"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout6"
                android:id="@+id/constraintLayout7">

            <View
                    android:id="@+id/view3"
                    android:layout_width="3dp"
                    android:layout_height="12dp"
                    android:layout_marginStart="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@color/black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_13"
                    android:textStyle="bold"
                    android:layout_marginStart="@dimen/dp_6"
                    android:text="操作细节"
                    app:layout_constraintStart_toStartOf="@+id/view3"
                    app:layout_constraintTop_toTopOf="@+id/view3"
                    app:layout_constraintBottom_toBottomOf="@+id/view3" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
                android:id="@+id/iv1"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginEnd="@dimen/dp_75"
                android:src="@mipmap/icon_wm_chart_heart_red"
                app:layout_constraintEnd_toStartOf="@+id/tvCycleCount1"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout7" />

        <TextView
                android:id="@+id/tvCycleCount1"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@drawable/progressbar_circle"
                android:gravity="center"
                android:text="0"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/iv1"
                app:layout_constraintBottom_toBottomOf="@+id/iv1" />

        <ImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginStart="@dimen/dp_60"
                android:src="@mipmap/icon_wm_chart_lung_red"
                app:layout_constraintStart_toEndOf="@+id/tvCycleCount1"
                app:layout_constraintTop_toTopOf="@+id/iv1"
                android:id="@+id/imageView6" />

        <TextView
                android:id="@+id/tvLungCount1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/press_result_desc"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintTop_toBottomOf="@+id/iv1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/iv1" />

        <TextView
                android:id="@+id/tvPrCount1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/press_result_number"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:layout_marginStart="@dimen/dp_10"
                app:layout_constraintStart_toEndOf="@+id/tvLungCount1"
                app:layout_constraintTop_toTopOf="@+id/tvLungCount1" />

        <TextView
                android:id="@+id/tvHeartCount1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/press_result_desc"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintStart_toEndOf="@+id/tvCycleCount1"
                app:layout_constraintTop_toBottomOf="@+id/tvCycleCount1" />

        <TextView
                android:id="@+id/tvQyCount1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/press_result_number"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:layout_marginStart="@dimen/dp_10"
                app:layout_constraintStart_toEndOf="@+id/tvHeartCount1"
                app:layout_constraintTop_toTopOf="@+id/tvHeartCount1" />

        <TextView
                android:id="@+id/tvLocation1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="位置错误：0次"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_2"
                android:textColor="@color/color_text_selected"
                android:textSize="10sp"
                app:layout_constraintTop_toBottomOf="@+id/tvLungCount1"
                app:layout_constraintStart_toStartOf="parent" />

        <TextView
                android:id="@+id/tvInsufficient1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="按压不足：0次"
                android:layout_marginStart="@dimen/dp_10"
                android:textColor="@color/color_text_selected"
                android:textSize="10sp"
                app:layout_constraintStart_toEndOf="@+id/tvLocation1"
                app:layout_constraintTop_toTopOf="@+id/tvLocation1" />

        <TextView
                android:id="@+id/tvPressBig1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="按压过大：0次"
                android:layout_marginTop="@dimen/dp_4"
                android:textColor="@color/color_text_selected"
                android:textSize="10sp"
                app:layout_constraintTop_toBottomOf="@+id/tvLocation1"
                app:layout_constraintStart_toStartOf="@+id/tvLocation1" />

        <TextView
                android:id="@+id/tvRebound1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="回弹不足：0次"
                android:textColor="@color/color_text_selected"
                android:layout_marginTop="@dimen/dp_4"
                android:textSize="10sp"
                app:layout_constraintTop_toBottomOf="@+id/tvLocation1"
                app:layout_constraintStart_toStartOf="@+id/tvInsufficient1" />

        <TextView
                android:id="@+id/tvOutBoutCount1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="按压超次：0次"
